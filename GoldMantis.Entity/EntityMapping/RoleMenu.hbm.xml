<?xml version="1.0" encoding="utf-8"?>
<hibernate-mapping assembly="GoldMantis.Entity" namespace="GoldMantis.Entity" xmlns="urn:nhibernate-mapping-2.2">
  <class name="RoleMenu" table="RoleMenu" lazy="true" >
    <id name="ID">
      <generator class="increment" />
    </id>
    <property name="RoleID"  >
      <column name="RoleID" sql-type="int" not-null="true" />
    </property>
    <property name="MenuOperationID"  >
      <column name="MenuOperationID" sql-type="int" not-null="true" />
    </property>
  </class>
</hibernate-mapping>
