<?xml version="1.0" encoding="utf-8"?>
<hibernate-mapping assembly="GoldMantis.Entity" namespace="GoldMantis.Entity" xmlns="urn:nhibernate-mapping-2.2">
  <class name="Menu" table="Menu" lazy="true" >
    <id name="ID">
      <generator class="increment" />
    </id>
    <property name="ParentID"  >
      <column name="ParentID" sql-type="int" not-null="true" />
    </property>
    <property name="ModuleID"  >
      <column name="ModuleID" sql-type="int" not-null="true" />
    </property>
    <property name="Name"  >
      <column name="Name" sql-type="nvarchar" not-null="true" />
    </property>
    <property name="Area"  >
      <column name="Area" sql-type="nvarchar" not-null="false" />
    </property>
    <property name="Controller"  >
      <column name="Controller" sql-type="nvarchar" not-null="true" />
    </property>
    <property name="Action"  >
      <column name="Action" sql-type="nvarchar" not-null="true" />
    </property>
    <property name="IconClass"  >
      <column name="IconClass" sql-type="nvarchar" not-null="false" />
    </property>
    <property name="MenuOrder"  >
      <column name="MenuOrder" sql-type="nvarchar" not-null="false" />
    </property>
    <property name="MenuLevel"  >
      <column name="MenuLevel" sql-type="int" not-null="true" />
    </property>
    <property name="NamePath"  >
      <column name="NamePath" sql-type="nvarchar" not-null="true" />
    </property>
    <property name="KeyPath"  >
      <column name="KeyPath" sql-type="nvarchar" not-null="true" />
    </property>
    <property name="Description"  >
      <column name="Description" sql-type="nvarchar" not-null="false" />
    </property>
    <property name="IsDelete"  >
      <column name="IsDelete" sql-type="bit" not-null="true" />
    </property>
    <property name="IsOn"  >
      <column name="IsOn" sql-type="bit" not-null="true" />
    </property>
  </class>
</hibernate-mapping>
